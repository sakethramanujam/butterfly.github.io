  "name": "hello-express",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
